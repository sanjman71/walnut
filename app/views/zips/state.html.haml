- title(@title)

.span-23.mat.prepend-1.last.padding-top-10.padding-bottom-20
  %h2= @h1
  %h4.span-7.last#breadcrumbs
    = render(:partial => 'shared/breadcrumbs', :locals => {:tree => 'zips', :country => @country, :state => @state, :city => @city, :zip => nil})

%hr.space
    
%h3.span-23.prepend-1
  - unless @zips.blank?
    .state_zips== #{@state.name} Zip Codes
    - @zips.each_with_index do |zip, i|
      - klasses = i.odd? ? ["locality", "odd",] : ["locality", "even"]
      %span.small[zip]= link_to(zip, url_for(:controller => 'zips', :action => 'zip', :country => @country, :state => @state, :zip => zip), :class => klasses.join(" "))
    
-#%h3.span-20.prepend-1
  -#== Browse Zip Codes by City
